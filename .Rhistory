library(agricolae)
library(tidyverse)
library(writexl)
library(readxl)
library(jtools)
library(plotly)
library(kableExtra)
orange_earth <- "#f37321"
green_earth <- "#008C99"
datos <- read_excel("datos/data_final.xlsx")
gender <- datos |>
group_by(Gender) |>
summarise(
N = n(),
Porcentaje = round(100*N/nrow(datos),2)
)
gender_country <- datos |>
group_by(Gender, Origin_Country) |>
summarise(
N = n(),
Porcentaje = round(100*N/nrow(datos),2)
) |>
ungroup()
#| label: fig-country
#| fig-cap: "Distribución de los participantes por país"
#| echo: false
country <- datos |>
group_by(Origin_Country) |>
summarise(
N = n(),
Porcentaje = round(100*N/nrow(datos),2)
)
g2 <- country |>
mutate(Origin_Country = fct_reorder(Origin_Country, N)) |>
ggplot( aes(x = Origin_Country, y = N)) +
geom_bar(stat = "identity", fill = orange_earth) +
coord_flip() +
theme_apa() +
labs(
x = "País", y = "Frecuencia"
)
ggplotly(g2)
